@model IEnumerable<BigFoot.UserData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Udata)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Content.ImagPath)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContentTypes.ContentType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Udata)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Content.ImagPath)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContentTypes.ContentType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UDID }) |
            @Html.ActionLink("Details", "Details", new { id=item.UDID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UDID })
        </td>
    </tr>
}

</table>
