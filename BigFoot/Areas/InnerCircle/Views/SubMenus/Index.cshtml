@model IEnumerable<BigFoot.SubMenus>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubMenu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.smUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Menus.Menu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SubMenu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.smUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Menus.Menu)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SubMenuID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SubMenuID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SubMenuID })
        </td>
    </tr>
}

</table>
