@model IEnumerable<BigFoot.Content>

<div id="titbg">
    <h2>@ViewBag.Title</h2>
</div>
@*<img src="~/Custom/foot.png" width="350px" id="Titlefoot" />*@


@{ 
    var eachRow = Model.Take(3);
}

@foreach (var p in Model)
{
    <div class="col-sm-4 col-xs-12 ">
        <div class="panel panel-default text-center ">
            

            <div class="panel-body EApic" style="background: url(@p.ImagPath) no-repeat; background-size:contain;">
                <div class="EAtext">
            @foreach (var d in p.UserDatas)
            {


                @Html.Raw(d.ContentTypes.PreHTML);
                @d.Udata;
                @Html.Raw(d.ContentTypes.PostHTML);

             
            
            }
                </div>
            </div>


            @*closing divs of row*@
        </div>
    </div>
}


